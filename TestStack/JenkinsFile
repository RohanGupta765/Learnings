node{
		stage('SCM Checkout')
		{	
			git "https://github.com/RohanGupta765/Learnings"
		}
		
		stage('Compile Package')
		{	
			def maven_path=tool name: 'M3', type: 'maven'
			sh "${maven_path}/bin/mvn package"
		}
		
		stage('Testing Stage')
		{
			def maven_path= tool name: 'M3', type: 'maven'
			sh "${maven_path}/bin/mvn test"
		}
	 
}	
